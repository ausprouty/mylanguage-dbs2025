{
  "name": "dbs2025",
  "version": "0.0.1",
  "description": "Discovering Spiritual Community",
  "productName": "Discovering Spiritual Community",
  "author": "ausprouty <bob.prouty@powertochange.org.au>",
  "private": true,
  "scripts": {
    "validate:tokens": "node scripts/validate-site-tokens.mjs",
    "predev": "npm run validate:tokens",
    "prebuild": "npm run validate:tokens",
    "clean": "rimraf --glob \"node_modules/.vite-*\" dist .quasar",
    "clean:site": "rimraf dist/site-%SITE% node_modules/.vite-%SITE%",
    "dev:dbs": "yarn clean:site && cross-env SITE=dbs quasar dev -m pwa",
    "build:dbs": "yarn clean:site && cross-env SITE=dbs quasar build -m pwa",
    "dev:uom": "yarn clean:site && cross-env SITE=uom quasar dev -m pwa",
    "build:uom": "yarn clean:site && cross-env SITE=uom quasar build -m pwa",
    "dev:guru": "yarn clean:site && cross-env SITE=guru quasar dev -m pwa",
    "build:guru": "yarn clean:site && cross-env SITE=guru quasar build -m pwa",
    "dev:wisdom": "yarn clean:site && cross-env SITE=wisdom quasar dev -m pwa",
    "build:wisdom": "yarn clean:site && cross-env SITE=wisdom quasar build -m pwa",
    "dev:wsu": "yarn clean:site && cross-env SITE=wsu quasar dev -m pwa",
    "build:wsu": "yarn clean:site && cross-env SITE=wsu quasar build -m pwa",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@google-cloud/translate": "^9.1.0",
    "@quasar/cli": "^2.3.0",
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "deepmerge": "^4.3.1",
    "idb": "^8.0.3",
    "joi": "^17.11.0",
    "lodash.debounce": "^4.0.8",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "quasar": "^2.6.0",
    "register-service-worker": "^1.7.2",
    "vue": "^3.0.0",
    "vue-html2pdf": "^1.8.0",
    "vue-i18n": "^11.0.0-rc.1",
    "vue-pdf": "^4.3.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@quasar/app-vite": "^2.3.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "cross-env": "^10.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "rimraf": "^6.0.1",
    "sass": "^1.90.0",
    "sass-embedded": "^1.90.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "webpack": "^5.97.1",
    "workbox-build": "6.5.0",
    "workbox-cacheable-response": "6.5.0",
    "workbox-core": "6.5.0",
    "workbox-expiration": "6.5.0",
    "workbox-precaching": "6.5.0",
    "workbox-routing": "6.5.0",
    "workbox-strategies": "6.5.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "packageManager": "yarn@4.9.1"
}
